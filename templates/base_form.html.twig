    <script>tinymce.init({selector:'.adminForm',});</script>

    <link rel="shortcut icon" href="{{asset('assets/img/ico/favicon.ico')}}">
    <link rel="apple-touch-icon" sizes="144x144" href="{{asset('assets/img/ico/apple-touch-icon-144x144.png')}}">
