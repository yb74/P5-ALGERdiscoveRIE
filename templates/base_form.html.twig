    <!-- Custom CSS -->
    <link href="{{asset('css/myStyle.css')}}" rel="stylesheet">

    {% block stylesheets %}{% endblock %}
</head>

<body>
    <div class="container">
        {% block body %}{% endblock %}

        {% block javascripts %}{% endblock %}
    </div>
